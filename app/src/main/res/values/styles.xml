<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="basic_radio_button">
        <item name="android:drawableStart">
            @drawable/basic_radio
        </item>
        <item name="android:button">
            @null
        </item>
    </style>
    <style name="basic_check_box">
        <item name="android:drawableStart">
            @drawable/basic_checkbox
        </item>
        <item name="android:button">
            @null
        </item>
    </style>
    <style name="edit_text">
        <item name="android:background">
            @drawable/basic_whitebox
        </item>
    </style>
    <style name="CalenderViewCustom" parent="Theme.AppCompat">
        <item name="colorAccent">@color/aquamarine_700</item>
        <item name="colorPrimary">@color/white</item>
        <item name="android:textColorPrimary">@color/black</item>
    </style>
    <style name="CalenderViewDateCustomText" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textColor">@color/black</item>
        <item name="android:weekNumberColor">@color/aquamarine_700</item>
    </style>
    <style name="CalenderViewWeekCustomText" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textColor">@color/black</item>
    </style>
</resources>